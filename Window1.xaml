<Window
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:local="clr-namespace:AVosSouhaits" x:Class="AVosSouhaits.Window1"
        Title="Window1" Height="300" Width="300" Loaded="Window_Loaded">
    <Window.Resources>
        <local:AVSDatabaseDataSet x:Key="aVSDatabaseDataSet"/>
        <CollectionViewSource x:Key="projetViewSource" Source="{Binding Projet, Source={StaticResource aVSDatabaseDataSet}}"/>
    </Window.Resources>
    <Grid DataContext="{StaticResource projetViewSource}"/>
</Window>
